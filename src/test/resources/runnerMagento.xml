<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="magento test">
    <parameter name="useCloudEnv" value="false"></parameter>

    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="10"></parameter>
    <parameter name="browserVersion" value="100"></parameter>

    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://magento.softwaretestingboard.com/"></parameter>
    <test name="magento test">
        <classes>
            <class name="com.Magento.test.TestSignin"></class>
            <class name="com.Magento.test.TestCreateNewAccount"></class>
            <class name="com.Magento.test.TestCheckOutCompareList"></class>
            <class name="com.Magento.test.TestCheckOutShoppingCart"></class>
            <class name="com.Magento.test.TestSearch"></class>
            <class name="com.Magento.test.TestSignOut"></class>
            <class name="com.Magento.test.TestFonctionsProductPage"></class>
            <class name="com.Magento.test.TestAddRatingAndReview"></class>
            <class name="com.Magento.test.TestReorderAndRuturn"></class>
            <class name="com.Magento.test.TestDownloadableProducts"></class>
        </classes>
    </test>
</suite>