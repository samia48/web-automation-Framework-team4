<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="scaledupit test">
    <parameter name="useCloudEnv" value="false"></parameter>

    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="10"></parameter>
    <parameter name="browserVersion" value="100"></parameter>

    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://automation.scaledupit.com/"></parameter>
    <test name="scaledupit test">
        <classes>
           <class name="com.Scaledupit.test.TestLogin"></class>
            <class name="com.Scaledupit.test.TestRegister"></class>
            <class name="com.Scaledupit.test.TestSearch"></class>
            <class name="com.Scaledupit.test.TestShoppingFromMenu"></class>
            <class name="com.Scaledupit.test.TestShoppingFromHomePage"></class>
            <class name="com.Scaledupit.test.ShoppingByCategories"></class>
            <class name="com.Scaledupit.test.TestAddReview"></class>
            <class name="com.Scaledupit.test.TestFooterFunctions"></class>
            <class name="com.Scaledupit.test.TestDiscountSales"></class>
            <class name="com.Scaledupit.test.TestConnectSocialMedia"></class>
            <class name="com.Scaledupit.test.TestLogOut"></class>
        </classes>
    </test>
</suite>