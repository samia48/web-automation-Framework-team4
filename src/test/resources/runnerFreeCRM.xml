<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="FreeCRM test">
    <parameter name="useCloudEnv" value="false"></parameter>

    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="windows"></parameter>
    <parameter name="osVersion" value="10"></parameter>
    <parameter name="browserVersion" value="100"></parameter>

    <parameter name="browserName" value="chrome"></parameter>
    <parameter name="url" value="https://ui.cogmento.com/?lang=en-GB"></parameter>
    <test name="FreeCRM tests">
        <classes>
            <class name="com.FreeCRM.test.LoginTest"></class>
            <class name="com.FreeCRM.test.BlankUsernameAndPasswordTest"></class>
            <class name="com.FreeCRM.test.InvalidEmailErrorMessageTest"></class>
            <class name="com.FreeCRM.test.InvalidPasswordErrorMessageTest"></class>
            <class name="com.FreeCRM.test.FetchLeftMenuTest"></class>

        </classes>
    </test>
</suite>